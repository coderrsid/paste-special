{
  "name": "joplin-plugin-pastespecial",
  "version": "1.0.0",
  "scripts": {
    "dist": "webpack --joplin-plugin-config buildMain && webpack --joplin-plugin-config buildExtraScripts && webpack --joplin-plugin-config createArchive",
    "prepare": "npm run dist",
    "update": "npm install -g generator-joplin && yo joplin --update",
    "test": "jest --watchAll --testPathPattern=tests"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/index.js"
    ],
    "coverageReporters": [
      "text"
    ]
  },
  "license": "MIT",
  "keywords": [
    "joplin-plugin"
  ],
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.14.5",
    "@types/jest": "^26.0.23",
    "@types/node": "^14.0.14",
    "chalk": "^4.1.0",
    "copy-webpack-plugin": "^6.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.2",
    "fs-extra": "^9.0.1",
    "glob": "^7.1.6",
    "jest": "^27.0.6",
    "on-build-webpack": "^0.1.0",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "tar": "^6.0.5",
    "ts-jest": "^27.0.3",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "yargs": "^16.2.0"
  },
  "dependencies": {
    "ignore-loader": "^0.1.2",
    "papaparse": "^5.3.1",
    "react": "^17.0.2"
  }
}
